<Page x:Class="WpfApp1.PizzaPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PizzaPage"
      Background="white">

    <Grid>
        <Grid.Background>
            <ImageBrush x:Name="Img_Fond" ImageSource="pack://siteoforigin:,,,/bin/Debug/pizzapage.jpg" Stretch="Fill" TileMode="None" />
        </Grid.Background>
        <WebBrowser x:Name="wbMain" Margin="223,184,0,-0.4" Source="pack://siteoforigin:,,,/pizzapage.html" LoadCompleted="loadCompletePagePizza">
            </WebBrowser>
        </Grid>
</Page>
